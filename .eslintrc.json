{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:prettier/recommended",
        "google"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": ["error",{
            "endOfLine": "auto"
        }],
        "linebreak-style": 0,
        "max-depth": ["error", 2],
        "require-jsdoc": 0,
        "object-curly-spacing": 0
    }
}
