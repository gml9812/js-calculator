{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:prettier/recommended",
        "google"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "prettier/prettier": ["error",{
            "endOfLine": "auto"
        }],
        "linebreak-style": 0,
        "max-depth": ["error", 1]
    }
}
